{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": "web",
  "installCommand": "",
  "framework": null,
  "rewrites": [
    {
      "source": "/",
      "destination": "/login.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/student/dashboard",
      "destination": "/student-dashboard.html"
    },
    {
      "source": "/admin/dashboard",
      "destination": "/admin-dashboard.html"
    },
    {
      "source": "/admin/users",
      "destination": "/users.html"
    },
    {
      "source": "/admin/reports",
      "destination": "/reports.html"
    },
    {
      "source": "/admin/settings",
      "destination": "/settings.html"
    },
    {
      "source": "/admin/complaints",
      "destination": "/complaints.html"
    },
    {
      "source": "/admin/announcements",
      "destination": "/announcements.html"
    },
    {
      "source": "/admin/complaint/:id",
      "destination": "/complaint-detail.html"
    },
    {
      "source": "/student/complaint/:id",
      "destination": "/student-complaint-detail.html"
    },
    {
      "source": "/complaint/submit",
      "destination": "/submit-complaint.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

